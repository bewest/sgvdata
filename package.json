{
  "name": "sgvdata",
  "version": "0.3.0-alpha",
  "description": "Utilities to deal with sensor glucose values.",
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nightscout/sgvdata.git"
  },
  "keywords": [
    "sgv",
    "cgm",
    "glucose",
    "format"
  ],
  "author": "Ben West",
  "bugs": {
    "url": "https://github.com/nightscout/sgvdata/issues"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node",
        "tests/compare"
      ],
      "pattern": [
        "tests",
        "lib",
        "index"
      ]
    }
  },
  "testling": {
    "harness": "mocha",
    "browsers": [
      "ie/9..latest",
      "chrome/20..latest",
      "firefox/14..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6"
    ],
    "files": [
      "tests/*.js"
    ]
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "should": "^13.2.3",
    "travis-cov": "^0.2.5"
  },
  "dependencies": {
    "event-stream": "^4.0.1",
    "protobufjs": "^5.0.3",
    "pug": "^2.0.3"
  }
}
